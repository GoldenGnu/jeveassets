										<include>*.*</include>
									</includes>
								</fileSet>
							</fileSets>
							<algorithms>
								<algorithm>MD5</algorithm>
							</algorithms>
							<csvSummary>false</csvSummary>
							<individualFiles>true</individualFiles>
						</configuration>
					</plugin>
					<plugin>
						<artifactId>maven-assembly-plugin</artifactId>
						<version>2.4</version>
