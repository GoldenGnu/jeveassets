							</execution>
						</executions>
					</plugin>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>exec-maven-plugin</artifactId>
						<version>1.4.0</version>
						<executions>
							<!-- jlink: JRE -->
							<execution>
								<id>package-1-jlink</id>
								<phase>verify</phase>
